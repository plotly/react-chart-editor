{
  "name": "react-chart-editor",
  "description": "plotly.js chart editor react component UI",
  "version": "0.35.6",
  "author": "Plotly, Inc.",
  "bugs": {
    "url": "https://github.com/plotly/react-chart-editor/issues"
  },
  "dependencies": {
    "@plotly/draft-js-export-html": "1.2.0",
